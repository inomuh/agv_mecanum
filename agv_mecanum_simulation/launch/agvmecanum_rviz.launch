<?xml version="1.0"?>
<launch> 
   <!-- values passed by command line input -->
   <arg name="model" value="$(find agv_mecanum_description)/urdf/agv_mecanum.urdf.xacro"/>
   <arg name="gui" default="false" />

   <!-- set these parameters on Parameter Server -->
   <param name="robot_description" command="$(find xacro)/xacro --inorder $(find agv_mecanum_description)/urdf/agv_mecanum.urdf.xacro" />
   <param name="use_gui" value="$(arg gui)" />

   <!-- Start only rviz node -->
   <node name="rviz" pkg="rviz" type="rviz" args="-d $(find agv_mecanum_simulation)/rviz/urdf.rviz" required="true"/> 
</launch>
